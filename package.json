{
  "name": "geomancer",
  "version": "3.0.0",
  "description": "A canvas drawing framework",
  "main": "src/index.js",
  "svelte": "src/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "ava",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mildewey/geomancer.git"
  },
  "author": "Mark Dewey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mildewey/geomancer/issues"
  },
  "homepage": "https://github.com/mildewey/geomancer#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "ava": "^3.10.1",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "sirv-cli": "^0.4.4",
    "svelte": "^3.0.0"
  },
  "dependencies": {},
  "ava": {
    "files": [
      "tests/**/*"
    ],
    "sources": [
      "src/**/*"
    ],
    "concurrency": 6,
    "failFast": true,
    "failWithoutAssertions": false,
    "verbose": true
  }
}
